    public void generatenewemp() throws SQLException {

        doConnect();
        String d = "SELECT * FROM employee ORDER BY empid  DESC LIMIT 1";

        resultSet = statement.executeQuery(d);
        resultSet.next();
        int x = Integer.parseInt(resultSet.getString("empid")) + 1;
        txt_id.setText(x + "");

    }
    private void cmd_saveActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int p = JOptionPane.showConfirmDialog(null, "Are you sure you want to add record?", "Add Record", JOptionPane.YES_NO_OPTION);
        if (p == 0) {
            LocalDate date1 = LocalDate.parse(txt_dob.getText());
            LocalDate date2 = LocalDate.parse(txt_doj.getText());
            LocalDate date3 = LocalDate.parse(endContract.getText());
            query = "INSERT INTO employee.employee (empid, `civilNo`, `Name`, `Nationalty`, sex, birthday, address, maobile, `Lname`, basicsalary, image) \n"
                    + "	VALUES (" + Integer.parseInt(txt_id.getText()) + "," + Integer.parseInt(txt_dep.getText()) + ",'" + txt_firstname.getText() + "','" + txt_address.getText() + "','" + 1 + "','" + date1 + "','" + txt_address2.getText() + "'\n"
                    + ",'" + txt_tel.getText() + "','" + txt_surname.getText() + "'," + Float.parseFloat(txt_salary.getText()) + ",'" + txt_id.getText() + "');";
            //, city, maobile, `Lnamr`, basicsalary, `dateHire`, image
            try {
                int x = statement.executeUpdate(query);
                if (x == 1) {
                    query = "INSERT INTO employee.empposition (id, empid, jobTitle,startjobdate, degree, endcontract, department) \n"
                            + "	VALUES (" + Integer.parseInt(txt_id.getText()) + "," + Integer.parseInt(txt_id.getText()) + ",'" + txt_job.getText() + "','" + date2 + "'," + (jComboBox1.getSelectedIndex() + 1) + ",'" + date3 + "','" + (dep.getSelectedIndex() + 1) + "');";

                    int y = statement.executeUpdate(query);
                    if (x == 1) {
                        JOptionPane.showMessageDialog(null, "Data is saved successfully");
                    } else {
                        JOptionPane.showMessageDialog(null, "Faild the data not saved ");
                    }

                } else {
                    JOptionPane.showMessageDialog(null, "Faild the data not saved ");

                }
            } catch (SQLException ex) {
                Logger.getLogger(addEmployee.class.getName()).log(Level.SEVERE, null, ex);
            }

        }


    }                                        

